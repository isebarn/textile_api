env_file = ''

###env
url = "http://127.0.0.1:5000/api/{}"
images = "images"
products = "products"
users = "users"
###env

get(url.format(images))

post(url.format(images), json={
	"url": "asd",
	"fields": {
		"god": 1
	}
}, name="image")


